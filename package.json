{
  "name": "hb-scrub",
  "version": "1.1.0",
  "description": "Remove EXIF, GPS, and other metadata from images and documents. Supports JPEG, PNG, WebP, GIF, SVG, TIFF, HEIC, AVIF, PDF, MP4/MOV, and RAW formats.",
  "type": "module",
  "main": "dist/hb-scrub.cjs",
  "module": "dist/hb-scrub.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hb-scrub": "./dist/hb-scrub.cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hb-scrub.js",
      "require": "./dist/hb-scrub.cjs"
    },
    "./heic": {
      "types": "./dist/heic.d.ts",
      "import": "./dist/hb-scrub.heic.js",
      "require": "./dist/hb-scrub.heic.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/hb-scrub.node.js",
      "require": "./dist/hb-scrub.node.cjs"
    },
    "./stream": {
      "types": "./dist/node-stream.d.ts",
      "import": "./dist/hb-scrub.stream.js",
      "require": "./dist/hb-scrub.stream.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "exif",
    "metadata",
    "privacy",
    "image",
    "jpeg",
    "png",
    "webp",
    "gif",
    "svg",
    "tiff",
    "heic",
    "avif",
    "pdf",
    "mp4",
    "mov",
    "dng",
    "raw",
    "gps",
    "strip",
    "remove",
    "anonymize",
    "redact",
    "scrub",
    "batch"
  ],
  "author": "Fabio Souto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fasouto/picscrub.git"
  },
  "bugs": {
    "url": "https://github.com/fasouto/picscrub/issues"
  },
  "homepage": "https://picscrub.com",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "ajv": "^6.14.0"
  }
}
